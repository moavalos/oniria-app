{
  "objects": {
    "led_1": {
      "light": "#FF00FF",
      "dark": "#fa6bfa"
    },
    "led_2": {
      "light": "#FFFFFF",
      "dark": "#e6decc"
    },
    "led_ball": {
      "light": "#FFFFFF",
      "dark": "#DDDDDD"
    },
    "light_bed": {
      "light": "#FFFFFF",
      "dark": "#e6decc"
    },
    "floor_led": {
      "light": "#FFFFFF",
      "dark": "#BBBBBB"
    },
    "pc_emissive": {
      "light": "#1AFFFF",
      "dark": "#0099AA"
    },
    "portal": {
      "light": "#FFFFFF",
      "dark": "#CCCCCC",
      "interceptable": true,
      "event": { "type": "none", "name": "none" },
      "lookAtOffset": [-1, 0.5, 4],
      "resalted": true,
      "colorResalted_light": "#FF0000",
      "colorResalted_dark": "#ff9d00"
    },
    "monitor": {
      "interceptable": true,
      "event": { "type": "none", "name": "none" },
      "lookAtOffset": [-1.5, 0.1, 1],
      "resalted": true,
      "colorResalted_light": "#FF0000",
      "colorResalted_dark": "#ff9d00"
    },
    "neon_text": {
      "light": "#FFFFFF",
      "dark": "#e6decc",
      "lookAtOffset": [-1.2, -1.0, 3]
    },
    "dog_handler": {
      "animation": {
        "target": "dog_handler",
        "type": "pendulum",
        "params": {
          "axis": [0, 1, 0],
          "angle": 30,
          "D_right": 0.8,
          "D_left": 1.2,
          "T_rest": 1.8
        },
        "loop": true
      },
      "lookAtOffset": [-1.2, 1.5, 1]
    },
    "top_chair": {
      "lookAtOffset": [-1.2, 1, 2.5]
    },
    "chair_handler": {
      "interceptable": true,
      "event": [
        {
          "type": "animation",
          "action": [
            {
              "on": "hoverEnter",
              "target": "chair_handler",
              "type": "rotateTo",
              "params": {
                "axis": [0, 1, 0],
                "to": 60,
                "duration": 0.6,
                "ease": "power2.out"
              },
              "loop": false,
              "autoPlay": false
            },
            {
              "on": "hoverLeave",
              "target": "chair_handler",
              "type": "rotateTo",
              "params": {
                "axis": [0, 1, 0],
                "to": 15,
                "duration": 0.8,
                "ease": "elastic.out(1, 0.5)"
              },
              "loop": false,
              "autoPlay": false
            }
          ]
        },
        {
          "type": "function",
          "action": [{ "on": "click", "function": "openChairDrawer" }]
        }
      ]
    },
    "cooler": {
      "animation": {
        "target": "cooler",
        "type": "rotate",
        "params": { "axis": [0, 0, 1], "angle": -360, "duration": 1 },
        "loop": true
      }
    }
  }
}
